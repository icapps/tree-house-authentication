{
  "name": "@icapps/tree-house-authentication",
  "version": "4.0.1",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup",
    "git:clean": "git branch --merged | egrep -v \"(^\\*|master|staging|develop)\" | xargs git branch -d",
    "git:clean-remote": "git remote prune origin"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "bcrypt": "~5.0.1",
    "express-session": "~1.17.2",
    "jsonwebtoken": "~8.5.1",
    "jwks-rsa": "~2.0.4",
    "ldapjs": "~2.3.0",
    "openid-client": "~4.7.4",
    "qrcode": "~1.4.4",
    "samlify": "~2.7.7",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/express-session": "~1.17.4",
    "@types/jest": "~26.0.24",
    "@types/jsonwebtoken": "~8.5.4",
    "@types/ldapjs": "~2.2.0",
    "@types/qrcode": "~1.4.1",
    "@types/speakeasy": "~2.0.6",
    "coveralls": "~3.1.1",
    "jest": "~27.0.6",
    "node-mocks-http": "~1.10.1",
    "np": "~7.5.0",
    "pre-commit": "~1.2.2",
    "supertest": "~6.1.4",
    "ts-jest": "~27.0.4",
    "tslint-config-airbnb": "~5.11.2",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5"
  },
  "engines": {
    "node": ">=14.x"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-authentication.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-authentication/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-authentication#readme"
}
