{
  "name": "tree-house-authentication",
  "version": "3.1.1",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "bcrypt": "~3.0.3",
    "express-session": "~1.15.6",
    "jsonwebtoken": "~8.4.0",
    "ldapjs": "~1.0.2",
    "qrcode": "~1.3.3",
    "samlify": "~2.5.0-rc1",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~3.0.0",
    "@types/express-session": "~1.15.11",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "~8.3.0",
    "@types/ldapjs": "~1.0.3",
    "@types/qrcode": "~1.3.0",
    "@types/speakeasy": "~2.0.3",
    "coveralls": "^3.0.2",
    "jest": "^24.0.0",
    "node-mocks-http": "^1.7.3",
    "np": "^4.0.2",
    "pre-commit": "^1.2.2",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
