{
  "name": "@icapps/tree-house-authentication",
  "version": "5.0.0",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "eslint ./src ./tests --ext .ts",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup",
    "git:clean": "git branch --merged | egrep -v \"(^\\*|master|staging|develop)\" | xargs git branch -d",
    "git:clean-remote": "git remote prune origin"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~5.48.1",
    "bcrypt": "~5.1.0",
    "eslint": "~8.32.0",
    "eslint-config-airbnb": "~19.0.4",
    "express-session": "~1.17.3",
    "jsonwebtoken": "~9.0.0",
    "jwks-rsa": "~3.0.1",
    "ldapjs": "~2.3.3",
    "openid-client": "~5.3.1",
    "qrcode": "~1.5.1",
    "samlify": "~2.8.7",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/express-session": "~1.17.5",
    "@types/jest": "~29.2.5",
    "@types/jsonwebtoken": "~9.0.1",
    "@types/ldapjs": "~2.2.5",
    "@types/qrcode": "~1.5.0",
    "@types/speakeasy": "~2.0.7",
    "coveralls": "~3.1.1",
    "jest": "~29.3.1",
    "node-mocks-http": "~1.12.1",
    "np": "~7.6.3",
    "pre-commit": "~1.2.2",
    "supertest": "~6.3.3",
    "ts-jest": "~29.0.5",
    "typescript": "~4.9.4"
  },
  "engines": {
    "node": ">=16.x"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-authentication.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-authentication/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-authentication#readme"
}
