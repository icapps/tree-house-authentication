{
  "name": "tree-house-authentication",
  "version": "3.1.4",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup",
    "git:clean": "git branch --merged | egrep -v \"(^\\*|master|staging|develop)\" | xargs git branch -d",
    "git:clean-remote": "git remote prune origin"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "bcrypt": "~3.0.6",
    "express-session": "~1.16.2",
    "jsonwebtoken": "~8.5.1",
    "ldapjs": "~1.0.2",
    "qrcode": "~1.3.3",
    "samlify": "^2.5.0",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~3.0.0",
    "@types/express-session": "~1.15.12",
    "@types/jest": "^24.0.14",
    "@types/jsonwebtoken": "~8.3.2",
    "@types/ldapjs": "~1.0.3",
    "@types/qrcode": "~1.3.3",
    "@types/speakeasy": "~2.0.4",
    "coveralls": "^3.0.4",
    "jest": "^24.8.0",
    "node-mocks-http": "^1.7.6",
    "np": "^5.0.3",
    "pre-commit": "^1.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-authentication.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-authentication/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-authentication#readme"
}
