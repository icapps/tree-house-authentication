{
  "name": "@icapps/tree-house-authentication",
  "version": "3.1.5",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup",
    "git:clean": "git branch --merged | egrep -v \"(^\\*|master|staging|develop)\" | xargs git branch -d",
    "git:clean-remote": "git remote prune origin"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "bcrypt": "~3.0.7",
    "express-session": "~1.17.0",
    "jsonwebtoken": "~8.5.1",
    "ldapjs": "~1.0.2",
    "qrcode": "~1.4.4",
    "samlify": "~2.6.2",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~3.0.0",
    "@types/express-session": "~1.15.16",
    "@types/jest": "~24.9.0",
    "@types/jsonwebtoken": "~8.3.6",
    "@types/ldapjs": "~1.0.4",
    "@types/qrcode": "~1.3.4",
    "@types/speakeasy": "~2.0.5",
    "coveralls": "~3.0.9",
    "jest": "~24.9.0",
    "node-mocks-http": "~1.8.1",
    "np": "~5.2.1",
    "pre-commit": "~1.2.2",
    "supertest": "~4.0.2",
    "ts-jest": "~24.3.0",
    "tslint-config-airbnb": "~5.11.2",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-authentication.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-authentication/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-authentication#readme"
}
